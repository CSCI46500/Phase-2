{
  "family": "model-registry-backend",
  "taskRoleArn": "arn:aws:iam::187641964754:role/ecsTaskExecutionRole-model-registry",
  "executionRoleArn": "arn:aws:iam::187641964754:role/ecsTaskExecutionRole-model-registry",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "PLACEHOLDER",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        { "name": "AWS_REGION", "value": "us-east-1" },
        { "name": "ADMIN_USERNAME", "value": "ece30861defaultadminuser" },
        { "name": "S3_BUCKET_NAME", "value": "modelregistry-models-187641964754" },
        { "name": "ENVIRONMENT", "value": "production" }
      ],
      "secrets": [
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:187641964754:secret:model-registry/database-url"
        },
        {
          "name": "ADMIN_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:187641964754:secret:model-registry/admin-password"
        },
        {
          "name": "SECRET_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:187641964754:secret:model-registry/secret-key"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/model-registry-backend",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": ["CMD-SHELL", "curl -f http://localhost:8000/health || exit 1"],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
